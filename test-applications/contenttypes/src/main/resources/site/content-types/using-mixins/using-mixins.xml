  <display-name>Using Mixins</display-name>
  <super-type>base:structured</super-type>
  <form>
    <item-set name="images">
      <label>Images</label>
      <items>
        <input name="label" type="TextLine">
          <label>Label</label>
          <occurrences minimum="0" maximum="1"/>
