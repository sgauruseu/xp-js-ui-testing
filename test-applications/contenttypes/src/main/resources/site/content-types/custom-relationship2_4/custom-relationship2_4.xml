<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <!-- This content type references custom relationship-types. -->
  <display-name>Custom Relationship</display-name>
  <super-type>base:structured</super-type>
  <form>
    <input name="cited-in" type="ContentSelector">
      <label>Cited In</label>
      <custom-text/>
      <help-text/>
      <occurrences minimum="2" maximum="4"/>
      <config>
        <relationship-type>com.enonic.xp.testing.contenttypes:citation</relationship-type>
      </config>
    </input>
  </form>
</content-type>
